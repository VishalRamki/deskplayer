  },
  "homepage": "https://github.com/VishalRamki/deskplayer#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@tauri-apps/api": "^1.0.0-beta.8",
    "music-metadata-browser": "^2.5.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "autoprefixer": "^10.3.4",
    "cssnano": "^5.0.8",
    "npm-run-all": "^4.1.5",
